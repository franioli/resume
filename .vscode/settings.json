{
  "latex-workshop.latex.tools": [
    // Compilation tools and commands
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-f",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOCFILE%"
      ]
    }
  ],

  "latex-workshop.latex.recipes": [
    {
      "name": "pdflatex",
      "tools": ["pdflatex"]
    }
  ],
  "editor.formatOnSave": false,
  "editor.formatOnType": false,
  "editor.formatOnPaste": false
}
